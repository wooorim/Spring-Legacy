<mapper namespace="edu.global.ex.board.one.IBDao"> <!-- namespace = 해당 인터페이스 이름 -->
   
   <select id="listDao" resultType="edu.global.ex.vo.BoardVO">
      <![CDATA[
         select * from mvc_board order by bGroup desc, bStep asc
      ]]>
   </select>
   

